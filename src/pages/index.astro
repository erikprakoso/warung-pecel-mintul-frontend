---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero.tsx";
import ProductHighlights from "../components/ProductHighlights.astro";
import Extras from "../components/Extras.astro";
import Features from "../components/Features.astro";
import Gallery from "../components/Gallery.tsx";
import TestimonialsLocation from "../components/TestimonialsLocation.astro";
import Footer from "../components/Footer.astro";
---

<Layout>
	<main>
		<Hero client:load />
		<Features />
		<ProductHighlights />
		<Extras />
		<Gallery client:visible />
		<TestimonialsLocation />
	</main>
	<Footer />
</Layout>
